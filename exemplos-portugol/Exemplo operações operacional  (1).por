programa {
  funcao inicio() {
    
     inteiro a, b

     escreva("Digite o valor para variável A: ")
     leia(a)

     escreva("Digite o valor para variável B: ")
     leia(b)

     escreva("Coparação:\n")
     escreva("a>b = ", (a>b),  "\n")
     escreva("a < b = ", (a<b), "\n")
     escreva("a!= b = ", (a!=b), "\n")
     escreva("a==b = ", (a==b), "\n")






  }
}
